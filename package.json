{
  "name": "astroauction",
  "version": "1.0.0",
  "description": "Astro Auction is both a web application to assist with live sale tactics & a static webpage for Lost And Found, a local business in Wahiawa that reuses and repurposes items and offers them at discount.",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "start": "node server.js",
    "lint": "eslint .",
    "format": "prettier -w .",
    "test": "cross-env NODE_ENV=test jest --runInBand"
  },
  "lint-staged": {
    "*.{js,jsx,json,md,ejs}": [
      "prettier -w"
    ],
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boobeh123/AstroAuction.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/boobeh123/AstroAuction/issues"
  },
  "homepage": "https://github.com/boobeh123/AstroAuction#readme",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.8.0",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^5.1.0",
    "dotenv": "^17.2.1",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "method-override": "^3.0.0",
    "mongoose": "^8.17.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "validator": "^13.15.26"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2"
  }
}
